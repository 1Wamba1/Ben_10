/**
 *
 * @author dalio
 */
public class Cat extends Shelter{
    
    String breed,color;
    boolean expired;
    
    
    public Cat(){
        
    }
    
    public Cat(String breed, String color){
        super();
        this.breed = breed; //food brand
        this.color = color; //ToF if expired
       
    }
    
    @Override
    public String toString(){
        return "The cat's ID: " + invID + ". You bought " + count + ". It cost $" 
                + price + ". Supply level: " + suplvl 
                + ". Cat's breed: " + breed + ". Cat's color: " + color; 
    }
    
    
    public String getBreed(String breed){
        return breed;
    }
    
    
    public String setBreed(String breed){
        return this.breed;
    }
    
    public String getColor(){
        return color;
    }
    
     public String setColor(){
        return this.color;
    }
     
     public boolean getExpired(boolean expired){
         return expired = expired;
     }
     
     public boolean setExpired(){
         return this.expired;
     }

    
}
//
