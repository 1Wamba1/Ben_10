package week_1;

/**
 *
 * @author dalio
 */
public class Dog extends Shelter{
    
    String breed, sound;
    
    
    public Dog(){
        
    }
    
    public Dog( String breed, String sound){
        super();
        this.breed = breed; //food brand
        this.sound = sound; //ToF if expired
       
    }
    
    @Override
    public String toString(){
        return "The dog's ID: " + invID + ". You bought " + count + ". It cost $" 
                + price + ". Supply level: " + suplvl 
                + ". Dog's breed: " + breed + ". Dog's sounds like: " + sound; 
    }
    
   
    
    
    
    public String getBreed(){
        return breed;
    }
    
    
    public String setBred(String bred){
        return this.breed;
    }
    
    public String getSound(){
        return sound;
    }
    
     public String setSound(){
        return this.sound;
    }

    
}
